{
  "name": "zstart",
  "description": "一个简洁起始页",
  "displayName": "zStart起始页",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite --port 8080",
    "build": "run-p type-check \"build-only {@}\" --",
